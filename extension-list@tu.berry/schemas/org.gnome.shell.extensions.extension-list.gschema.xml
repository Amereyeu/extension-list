<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extension-extension-list">
    <schema path="/org/gnome/shell/extensions/extension-list/" id="org.gnome.shell.extensions.extension-list">
        <key type="u" name="button-icon">
            <range min="0" max="2"/>
            <default>0</default>
            <summary>button icon: 0 - prefs, 1 - delete, 2 - url</summary>
        </key>
        <key type="b" name="hide-disabled">
            <default>false</default>
            <summary>hide disabled in menu</summary>
        </key>
        <key type="b" name="debug-button">
            <default>false</default>
            <summary>show debug button</summary>
        </key>
        <key type="b" name="unpin-button">
            <default>false</default>
            <summary>show unpin button</summary>
        </key>
        <key type="as" name="unpin-list">
            <default>[]</default>
            <summary>list of unpinned extensions</summary>
        </key>
    </schema>
</schemalist>
